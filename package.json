{
  "name": "qwe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve host -o\" \"ng serve instance\"",
    "build": "ng build host --configuration production && ng build instance --configuration production",
    "watch": "concurrently \"ng build host --watch\" \"ng build instance --watch\" \"node server.js\"",
    "serve": "node server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.7",
    "@angular/common": "^18.2.7",
    "@angular/compiler": "^18.2.7",
    "@angular/core": "^18.2.7",
    "@angular/forms": "^18.2.7",
    "@angular/platform-browser": "18.2.7",
    "@angular/platform-browser-dynamic": "^18.2.7",
    "@angular/router": "^18.2.7",
    "es-module-shims": "^1.10.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.7.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-architects/native-federation": "^18.2.2",
    "@angular-devkit/build-angular": "^18.2.7",
    "@angular/cli": "^18.2.7",
    "@angular/compiler-cli": "^18.2.7",
    "@types/node": "*",
    "concurrently": "^9.0.1",
    "ng-packagr": "^18.2.1",
    "typescript": "~5.5.4"
  },
  "engines": {
    "node": ">=18"
  }
