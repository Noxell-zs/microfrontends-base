{
  "name": "qwe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve host -o\" \"ng serve instance\"",
    "build": "ng build host --configuration production && ng build instance --configuration production",
    "watch": "concurrently \"ng build host --watch\" \"ng build instance --watch\" \"node server.js\"",
    "serve": "node server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "es-module-shims": "^1.5.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-architects/native-federation": "^17.1.1",
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular/cli": "^17.1.1",
    "@angular/compiler-cli": "^17.1.0",
    "concurrently": "^8.2.2",
    "ng-packagr": "^17.1.0",
    "typescript": "~5.3.2"
  }
}
