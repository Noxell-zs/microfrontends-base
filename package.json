{
  "name": "qwe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve host -o\" \"ng serve instance\"",
    "build": "ng build host --configuration production && ng build instance --configuration production",
    "watch": "concurrently \"ng build host --watch\" \"ng build instance --watch\" \"node server.js\"",
    "serve": "node server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "es-module-shims": "^1.10.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-architects/native-federation": "^17.1.8",
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.12",
    "@types/node": "*",
    "concurrently": "^8.2.2",
    "ng-packagr": "^17.3.0",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
